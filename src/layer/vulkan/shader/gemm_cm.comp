            const uint Kd4_N_USGN_USGK_d_subgroupsize = (Kd4_N_USGN_USGK + (ncnn_subgroupSize * UNROLL_WG_M - 1)) / (ncnn_subgroupSize * UNROLL_WG_M);
            [[unroll]] for (uint q = 0; q < Kd4_N_USGN_USGK_d_subgroupsize; q++)
            {
                const uint siq = (q * UNROLL_WG_M + sgmi) * ncnn_subgroupSize + si;

                if (Kd4_N_USGN_USGK % (ncnn_subgroupSize * UNROLL_WG_M) == 0 || siq < Kd4_N_USGN_USGK)
                {
                    const uint zk = siq / (Kd4 * N * UNROLL_SG_N);
                    const uint znij = siq % (Kd4 * N * UNROLL_SG_N);
                    const uint zn = znij / (Kd4 * N);
