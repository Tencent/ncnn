                [[unroll]] for (uint zn = 0; zn < UNROLL_SG_N; zn++)
                {
                    if (transB == 0)
                    {
#if ncnn_VK_KHR_cooperative_matrix
                        coopMatLoad(B[zn], tmp_b[sgni], (zk * UNROLL_SG_N + zn) * (Nd4p * K), Nd4p, gl_CooperativeMatrixLayoutRowMajor);
#elif ncnn_VK_NV_cooperative_matrix
