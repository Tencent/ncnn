                [[unroll]] for (uint q = 0; q < Kd4_N_USGN_USGK_d_subgroupsize; q++)
                {
                    const uint siq = (q * UNROLL_WG_M + sgmi) * ncnn_subgroupSize + si;

                    if (Kd4_N_USGN_USGK % (ncnn_subgroupSize * UNROLL_WG_M) == 0 || siq < Kd4_N_USGN_USGK)
                    {
                        const uint j = siq / Kd4;
                        const uint i = siq % Kd4;

                        tmp_b[sgni][j * Kd4p + i] = prefetch_tmp_b[q];
