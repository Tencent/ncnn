                top_blob_fp16p_data[vv_offset.g] = float(v[1].g);
                top_blob_fp16p_data[vv_offset.b] = float(v[1].b);
                top_blob_fp16p_data[vv_offset.a] = float(v[1].a);
            }
#if NCNN_fp16_storage
            if (cast_type_to == 3)
            {
                top_blob_fp16s_data[v_offset.r] = float16_t(v[0].r);
