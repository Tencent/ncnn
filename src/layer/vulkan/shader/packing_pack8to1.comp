                image3d_st1(top_blob_3d, ivec3(gx, gy, z4 + 6), v[1].b);
                image3d_st1(top_blob_3d, ivec3(gx, gy, z4 + 7), v[1].a);
            }
            if (cast_type_to == 1)
            {
                image3d_st1(top_blob_3d_fp32, ivec3(gx, gy, z4 + 0), v[0].r);
                image3d_st1(top_blob_3d_fp32, ivec3(gx, gy, z4 + 1), v[0].g);
