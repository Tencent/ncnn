#version 450

#if NCNN_fp16_storage
#extension GL_EXT_shader_16bit_storage: require
#if !NCNN_fp16_arithmetic
struct sfpvec8 { f16vec4 abcd; f16vec4 efgh; };
#endif
#endif
#if NCNN_fp16_arithmetic
#extension GL_EXT_shader_explicit_arithmetic_types_float16: require
#endif

layout (constant_id = 0) const int cast_type_from = 0;
layout (constant_id = 1) const int cast_type_to = 0;
layout (constant_id = 2) const int storage_type_from = 0;
layout (constant_id = 3) const int storage_type_to = 0;

#define shape_constant_id_offset 4
layout (constant_id = shape_constant_id_offset + 0) const int dims = 0;
layout (constant_id = shape_constant_id_offset + 1) const int w = 0;
layout (constant_id = shape_constant_id_offset + 2) const int h = 0;
layout (constant_id = shape_constant_id_offset + 3) const int c = 0;
layout (constant_id = shape_constant_id_offset + 4) const int cstep = 0;

layout (constant_id = shape_constant_id_offset + 5) const int outdims = 0;
layout (constant_id = shape_constant_id_offset + 6) const int outw = 0;
layout (constant_id = shape_constant_id_offset + 7) const int outh = 0;
layout (constant_id = shape_constant_id_offset + 8) const int outc = 0;
layout (constant_id = shape_constant_id_offset + 9) const int outcstep = 0;

layout (local_size_x_id = 233) in;
layout (local_size_y_id = 234) in;
layout (local_size_z_id = 235) in;

layout (binding = 0) readonly buffer bottom_blob { sfpvec8 bottom_blob_data[]; };
layout (binding = 0) readonly buffer bottom_blob_fp32 { mat2x4 bottom_blob_fp32_data[]; };
layout (binding = 0) readonly buffer bottom_blob_fp16p { uvec4 bottom_blob_fp16p_data[]; };
#if NCNN_fp16_storage
layout (binding = 0) readonly buffer bottom_blob_fp16s { sfpvec8 bottom_blob_fp16s_data[]; };
#endif
layout (binding = 1) writeonly buffer top_blob { sfp top_blob_data[]; };
layout (binding = 1) writeonly buffer top_blob_fp32 { float top_blob_fp32_data[]; };
layout (binding = 1) writeonly buffer top_blob_fp16p { float top_blob_fp16p_data[]; };
#if NCNN_fp16_storage
layout (binding = 1) writeonly buffer top_blob_fp16s { sfp top_blob_fp16s_data[]; };
#endif
#if NCNN_image_shader
layout (binding = 2) uniform unfp sampler1D bottom_blob_1d;
layout (binding = 2) uniform unfp sampler2D bottom_blob_2d;
layout (binding = 2) uniform unfp sampler3D bottom_blob_3d;
layout (binding = 2) uniform highp sampler1D bottom_blob_1d_fp32;
layout (binding = 2) uniform highp sampler2D bottom_blob_2d_fp32;
layout (binding = 2) uniform highp sampler3D bottom_blob_3d_fp32;
layout (binding = 2) uniform mediump sampler1D bottom_blob_1d_fp16p;
layout (binding = 2) uniform mediump sampler2D bottom_blob_2d_fp16p;
layout (binding = 2) uniform mediump sampler3D bottom_blob_3d_fp16p;
#if NCNN_fp16_storage
layout (binding = 2) uniform mediump sampler1D bottom_blob_1d_fp16s;
layout (binding = 2) uniform mediump sampler2D bottom_blob_2d_fp16s;
layout (binding = 2) uniform mediump sampler3D bottom_blob_3d_fp16s;
#endif
layout (binding = 3, imfmtc1) writeonly uniform unfp image1D top_blob_1d;
layout (binding = 3, imfmtc1) writeonly uniform unfp image2D top_blob_2d;
layout (binding = 3, imfmtc1) writeonly uniform unfp image3D top_blob_3d;
layout (binding = 3, r32f) writeonly uniform highp image1D top_blob_1d_fp32;
layout (binding = 3, r32f) writeonly uniform highp image2D top_blob_2d_fp32;
layout (binding = 3, r32f) writeonly uniform highp image3D top_blob_3d_fp32;
layout (binding = 3, r32f) writeonly uniform mediump image1D top_blob_1d_fp16p;
layout (binding = 3, r32f) writeonly uniform mediump image2D top_blob_2d_fp16p;
layout (binding = 3, r32f) writeonly uniform mediump image3D top_blob_3d_fp16p;
#if NCNN_fp16_storage
layout (binding = 3, r16f) writeonly uniform mediump image1D top_blob_1d_fp16s;
layout (binding = 3, r16f) writeonly uniform mediump image2D top_blob_2d_fp16s;
layout (binding = 3, r16f) writeonly uniform mediump image3D top_blob_3d_fp16s;
#endif
#endif

layout (push_constant) uniform parameter
{
    int dims;
    int w;
    int h;
    int c;
    int cstep;

    int outdims;
    int outw;
    int outh;
    int outc;
    int outcstep;
} p;

void main()
{
    int gx = int(gl_GlobalInvocationID.x);
    int gy = int(gl_GlobalInvocationID.y);
    int gz = int(gl_GlobalInvocationID.z);

    if (gx >= psc(w) || gy >= psc(h) || gz >= psc(c))
        return;

    afpvec8 v;

    if (psc(dims) == 1)
    {
        if (storage_type_from == 0)
        {
            int gi = gx;

            if (cast_type_from == 0)
            {
                v = buffer_ld8(bottom_blob_data, gi);
            }
            if (cast_type_from == 1)
            {
                v = afpvec8(bottom_blob_fp32_data[gi]);
            }
            if (cast_type_from == 2)
            {
                v = afpvec8(mat2x4(vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].r),unpackHalf2x16(bottom_blob_fp16p_data[gi].g)),vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].b),unpackHalf2x16(bottom_blob_fp16p_data[gi].a))));
            }
#if NCNN_fp16_storage
            if (cast_type_from == 3)
            {
#if NCNN_fp16_arithmetic
                v = afpvec8(bottom_blob_fp16s_data[gi]);
#else
                v[0] = afpvec4(bottom_blob_fp16s_data[gi].abcd);
                v[1] = afpvec4(bottom_blob_fp16s_data[gi].efgh);
#endif
            }
#endif
        }
#if NCNN_image_shader
        if (storage_type_from == 1)
        {
            if (cast_type_from == 0)
            {
                v = image1d_ld8(bottom_blob_1d, gx);
            }
            if (cast_type_from == 1)
            {
                v = afpvec8(texelFetch(bottom_blob_1d_fp32, gx * 2, 0), texelFetch(bottom_blob_1d_fp32, gx * 2 + 1, 0));
            }
            if (cast_type_from == 2)
            {
                v = afpvec8(texelFetch(bottom_blob_1d_fp16p, gx * 2, 0), texelFetch(bottom_blob_1d_fp16p, gx * 2 + 1, 0));
            }
#if NCNN_fp16_storage
            if (cast_type_from == 3)
            {
                v = afpvec8(texelFetch(bottom_blob_1d_fp16s, gx * 2, 0), texelFetch(bottom_blob_1d_fp16s, gx * 2 + 1, 0));
            }
#endif
        }
#endif

        if (storage_type_to == 0)
        {
            ivec4 x4 = ivec4(gx * 8) + ivec4(0, 1, 2, 3);

            ivec4 v_offset = x4;
            ivec4 vv_offset = x4 + 4;

            if (cast_type_to == 0)
            {
                buffer_st1(top_blob_data, v_offset.r, v[0].r);
                buffer_st1(top_blob_data, v_offset.g, v[0].g);
                buffer_st1(top_blob_data, v_offset.b, v[0].b);
                buffer_st1(top_blob_data, v_offset.a, v[0].a);
                buffer_st1(top_blob_data, vv_offset.r, v[1].r);
                buffer_st1(top_blob_data, vv_offset.g, v[1].g);
                buffer_st1(top_blob_data, vv_offset.b, v[1].b);
                buffer_st1(top_blob_data, vv_offset.a, v[1].a);
            }
            if (cast_type_to == 1)
            {
                top_blob_fp32_data[v_offset.r] = float(v[0].r);
                top_blob_fp32_data[v_offset.g] = float(v[0].g);
                top_blob_fp32_data[v_offset.b] = float(v[0].b);
                top_blob_fp32_data[v_offset.a] = float(v[0].a);
                top_blob_fp32_data[vv_offset.r] = float(v[1].r);
                top_blob_fp32_data[vv_offset.g] = float(v[1].g);
                top_blob_fp32_data[vv_offset.b] = float(v[1].b);
                top_blob_fp32_data[vv_offset.a] = float(v[1].a);
            }
            if (cast_type_to == 2)
            {
                top_blob_fp16p_data[v_offset.r] = float(v[0].r);
                top_blob_fp16p_data[v_offset.g] = float(v[0].g);
                top_blob_fp16p_data[v_offset.b] = float(v[0].b);
                top_blob_fp16p_data[v_offset.a] = float(v[0].a);
                top_blob_fp16p_data[vv_offset.r] = float(v[1].r);
                top_blob_fp16p_data[vv_offset.g] = float(v[1].g);
                top_blob_fp16p_data[vv_offset.b] = float(v[1].b);
                top_blob_fp16p_data[vv_offset.a] = float(v[1].a);
            }
#if NCNN_fp16_storage
            if (cast_type_to == 3)
            {
                top_blob_fp16s_data[v_offset.r] = float16_t(v[0].r);
                top_blob_fp16s_data[v_offset.g] = float16_t(v[0].g);
                top_blob_fp16s_data[v_offset.b] = float16_t(v[0].b);
                top_blob_fp16s_data[v_offset.a] = float16_t(v[0].a);
                top_blob_fp16s_data[vv_offset.r] = float16_t(v[1].r);
                top_blob_fp16s_data[vv_offset.g] = float16_t(v[1].g);
                top_blob_fp16s_data[vv_offset.b] = float16_t(v[1].b);
                top_blob_fp16s_data[vv_offset.a] = float16_t(v[1].a);
            }
#endif
        }
#if NCNN_image_shader
        if (storage_type_to == 1)
        {
            int x4 = gx * 8;

            if (cast_type_to == 0)
            {
                image1d_st1(top_blob_1d, x4 + 0, v[0].r);
                image1d_st1(top_blob_1d, x4 + 1, v[0].g);
                image1d_st1(top_blob_1d, x4 + 2, v[0].b);
                image1d_st1(top_blob_1d, x4 + 3, v[0].a);
                image1d_st1(top_blob_1d, x4 + 4, v[1].r);
                image1d_st1(top_blob_1d, x4 + 5, v[1].g);
                image1d_st1(top_blob_1d, x4 + 6, v[1].b);
                image1d_st1(top_blob_1d, x4 + 7, v[1].a);
            }
            if (cast_type_to == 1)
            {
                image1d_st1(top_blob_1d_fp32, x4 + 0, v[0].r);
                image1d_st1(top_blob_1d_fp32, x4 + 1, v[0].g);
                image1d_st1(top_blob_1d_fp32, x4 + 2, v[0].b);
                image1d_st1(top_blob_1d_fp32, x4 + 3, v[0].a);
                image1d_st1(top_blob_1d_fp32, x4 + 4, v[1].r);
                image1d_st1(top_blob_1d_fp32, x4 + 5, v[1].g);
                image1d_st1(top_blob_1d_fp32, x4 + 6, v[1].b);
                image1d_st1(top_blob_1d_fp32, x4 + 7, v[1].a);
            }
            if (cast_type_to == 2)
            {
                image1d_st1(top_blob_1d_fp16p, x4 + 0, v[0].r);
                image1d_st1(top_blob_1d_fp16p, x4 + 1, v[0].g);
                image1d_st1(top_blob_1d_fp16p, x4 + 2, v[0].b);
                image1d_st1(top_blob_1d_fp16p, x4 + 3, v[0].a);
                image1d_st1(top_blob_1d_fp16p, x4 + 4, v[1].r);
                image1d_st1(top_blob_1d_fp16p, x4 + 5, v[1].g);
                image1d_st1(top_blob_1d_fp16p, x4 + 6, v[1].b);
                image1d_st1(top_blob_1d_fp16p, x4 + 7, v[1].a);
            }
#if NCNN_fp16_storage
            if (cast_type_to == 3)
            {
                image1d_st1(top_blob_1d_fp16s, x4 + 0, v[0].r);
                image1d_st1(top_blob_1d_fp16s, x4 + 1, v[0].g);
                image1d_st1(top_blob_1d_fp16s, x4 + 2, v[0].b);
                image1d_st1(top_blob_1d_fp16s, x4 + 3, v[0].a);
                image1d_st1(top_blob_1d_fp16s, x4 + 4, v[1].r);
                image1d_st1(top_blob_1d_fp16s, x4 + 5, v[1].g);
                image1d_st1(top_blob_1d_fp16s, x4 + 6, v[1].b);
                image1d_st1(top_blob_1d_fp16s, x4 + 7, v[1].a);
            }
#endif
        }
#endif
    }
    else if (psc(dims) == 2)
    {
        if (storage_type_from == 0)
        {
            int gi = gy * psc(w) + gx;

            if (cast_type_from == 0)
            {
                v = buffer_ld8(bottom_blob_data, gi);
            }
            if (cast_type_from == 1)
            {
                v = afpvec8(bottom_blob_fp32_data[gi]);
            }
            if (cast_type_from == 2)
            {
                v = afpvec8(mat2x4(vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].r),unpackHalf2x16(bottom_blob_fp16p_data[gi].g)),vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].b),unpackHalf2x16(bottom_blob_fp16p_data[gi].a))));
            }
#if NCNN_fp16_storage
            if (cast_type_from == 3)
            {
#if NCNN_fp16_arithmetic
                v = afpvec8(bottom_blob_fp16s_data[gi]);
#else
                v[0] = afpvec4(bottom_blob_fp16s_data[gi].abcd);
                v[1] = afpvec4(bottom_blob_fp16s_data[gi].efgh);
#endif
            }
#endif
        }
#if NCNN_image_shader
        if (storage_type_from == 1)
        {
            if (cast_type_from == 0)
            {
                v = image2d_ld8(bottom_blob_2d, ivec2(gx, gy));
            }
            if (cast_type_from == 1)
            {
                v = afpvec8(texelFetch(bottom_blob_2d_fp32, ivec2(gx * 2, gy), 0), texelFetch(bottom_blob_2d_fp32, ivec2(gx * 2 + 1, gy), 0));
            }
            if (cast_type_from == 2)
            {
                v = afpvec8(texelFetch(bottom_blob_2d_fp16p, ivec2(gx * 2, gy), 0), texelFetch(bottom_blob_2d_fp16p, ivec2(gx * 2 + 1, gy), 0));
            }
#if NCNN_fp16_storage
            if (cast_type_from == 3)
            {
                v = afpvec8(texelFetch(bottom_blob_2d_fp16s, ivec2(gx * 2, gy), 0), texelFetch(bottom_blob_2d_fp16s, ivec2(gx * 2 + 1, gy), 0));
            }
#endif
        }
#endif

        if (storage_type_to == 0)
        {
            ivec4 y4 = ivec4(gy * 8) + ivec4(0, 1, 2, 3);

            ivec4 v_offset = y4 * psc(outw) + gx;
            ivec4 vv_offset = (y4 + 4) * psc(outw) + gx;

            if (cast_type_to == 0)
            {
                buffer_st1(top_blob_data, v_offset.r, v[0].r);
                buffer_st1(top_blob_data, v_offset.g, v[0].g);
                buffer_st1(top_blob_data, v_offset.b, v[0].b);
                buffer_st1(top_blob_data, v_offset.a, v[0].a);
                buffer_st1(top_blob_data, vv_offset.r, v[1].r);
                buffer_st1(top_blob_data, vv_offset.g, v[1].g);
                buffer_st1(top_blob_data, vv_offset.b, v[1].b);
                buffer_st1(top_blob_data, vv_offset.a, v[1].a);
            }
            if (cast_type_to == 1)
            {
                top_blob_fp32_data[v_offset.r] = float(v[0].r);
                top_blob_fp32_data[v_offset.g] = float(v[0].g);
                top_blob_fp32_data[v_offset.b] = float(v[0].b);
                top_blob_fp32_data[v_offset.a] = float(v[0].a);
                top_blob_fp32_data[vv_offset.r] = float(v[1].r);
                top_blob_fp32_data[vv_offset.g] = float(v[1].g);
                top_blob_fp32_data[vv_offset.b] = float(v[1].b);
                top_blob_fp32_data[vv_offset.a] = float(v[1].a);
            }
            if (cast_type_to == 2)
            {
                top_blob_fp16p_data[v_offset.r] = float(v[0].r);
                top_blob_fp16p_data[v_offset.g] = float(v[0].g);
                top_blob_fp16p_data[v_offset.b] = float(v[0].b);
                top_blob_fp16p_data[v_offset.a] = float(v[0].a);
                top_blob_fp16p_data[vv_offset.r] = float(v[1].r);
                top_blob_fp16p_data[vv_offset.g] = float(v[1].g);
                top_blob_fp16p_data[vv_offset.b] = float(v[1].b);
                top_blob_fp16p_data[vv_offset.a] = float(v[1].a);
            }
#if NCNN_fp16_storage
            if (cast_type_to == 3)
            {
                top_blob_fp16s_data[v_offset.r] = float16_t(v[0].r);
                top_blob_fp16s_data[v_offset.g] = float16_t(v[0].g);
                top_blob_fp16s_data[v_offset.b] = float16_t(v[0].b);
                top_blob_fp16s_data[v_offset.a] = float16_t(v[0].a);
                top_blob_fp16s_data[vv_offset.r] = float16_t(v[1].r);
                top_blob_fp16s_data[vv_offset.g] = float16_t(v[1].g);
                top_blob_fp16s_data[vv_offset.b] = float16_t(v[1].b);
                top_blob_fp16s_data[vv_offset.a] = float16_t(v[1].a);
            }
#endif
        }
#if NCNN_image_shader
        if (storage_type_to == 1)
        {
            int y4 = gy * 8;

            if (cast_type_to == 0)
            {
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 0), v[0].r);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 1), v[0].g);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 2), v[0].b);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 3), v[0].a);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 4), v[1].r);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 5), v[1].g);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 6), v[1].b);
                image2d_st1(top_blob_2d, ivec2(gx, y4 + 7), v[1].a);
            }
            if (cast_type_to == 1)
            {
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 0), v[0].r);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 1), v[0].g);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 2), v[0].b);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 3), v[0].a);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 4), v[1].r);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 5), v[1].g);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 6), v[1].b);
                image2d_st1(top_blob_2d_fp32, ivec2(gx, y4 + 7), v[1].a);
            }
            if (cast_type_to == 2)
            {
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 0), v[0].r);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 1), v[0].g);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 2), v[0].b);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 3), v[0].a);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 4), v[1].r);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 5), v[1].g);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 6), v[1].b);
                image2d_st1(top_blob_2d_fp16p, ivec2(gx, y4 + 7), v[1].a);
            }
#if NCNN_fp16_storage
            if (cast_type_to == 3)
            {
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 0), v[0].r);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 1), v[0].g);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 2), v[0].b);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 3), v[0].a);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 4), v[1].r);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 5), v[1].g);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 6), v[1].b);
                image2d_st1(top_blob_2d_fp16s, ivec2(gx, y4 + 7), v[1].a);
            }
#endif
        }
#endif
    }
    else // if (psc(dims) == 3)
    {
        if (storage_type_from == 0)
        {
            int gi = gz * psc(cstep) + gy * psc(w) + gx;

            if (cast_type_from == 0)
            {
                v = buffer_ld8(bottom_blob_data, gi);
            }
            if (cast_type_from == 1)
            {
                v = afpvec8(bottom_blob_fp32_data[gi]);
            }
            if (cast_type_from == 2)
            {
                v = afpvec8(mat2x4(vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].r),unpackHalf2x16(bottom_blob_fp16p_data[gi].g)),vec4(unpackHalf2x16(bottom_blob_fp16p_data[gi].b),unpackHalf2x16(bottom_blob_fp16p_data[gi].a))));
