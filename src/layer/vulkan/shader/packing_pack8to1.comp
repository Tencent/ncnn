                top_blob_fp16s_data[v_offset.b] = float16_t(v[0].b);
                top_blob_fp16s_data[v_offset.a] = float16_t(v[0].a);
                top_blob_fp16s_data[vv_offset.r] = float16_t(v[1].r);
                top_blob_fp16s_data[vv_offset.g] = float16_t(v[1].g);
                top_blob_fp16s_data[vv_offset.b] = float16_t(v[1].b);
                top_blob_fp16s_data[vv_offset.a] = float16_t(v[1].a);
